            ],
            "meta": {
              "custom": {
                "customCode": "const deviceVar = '$device';\nlet statusVar = [];\nconst valueVar = '$value';\n\ntry {\n  statusVar = JSON.parse('${status:json}')\n} catch { }\n\nconst createdMs = new Date().valueOf();\nconst oneDayDiffMs = 1000 * 60 * 60 * 24;\n\nconst items = [\n  {\n    created: createdMs,\n    name: 'a',\n    status: 'active',\n    value: 10,\n  },\n  {\n    created: createdMs - oneDayDiffMs,\n    name: 'b',\n    status: 'pending',\n    value: 12,\n  },\n  {\n    created: createdMs - oneDayDiffMs * 2,\n    name: 'c',\n    status: 'inactive',\n    value: 30,\n  },\n  {\n    created: createdMs - oneDayDiffMs * 2,\n    name: 'aa',\n    status: 'active',\n    value: 10,\n  },\n  {\n    created: createdMs,\n    name: 'bb',\n    status: 'pending',\n    value: 13,\n  },\n  {\n    created: createdMs - oneDayDiffMs,\n    name: 'cc',\n    status: 'inactive',\n    value: 32,\n  },\n];\n\nconst filteredItems = items.filter((item) => {\n  if (deviceVar && !item.name.includes(deviceVar)) {\n    return false;\n  }\n\n  if (statusVar.length && !statusVar.includes(item.status)) {\n    return false;\n  }\n\n  if (valueVar && Number(valueVar) !== item.value) {\n    return false;\n  }\n\n  return true;\n});\n\nconst fieldValues = {\n  created: [],\n  name: [],\n  status: [],\n  value: [],\n};\n\nfor (const item of filteredItems) {\n  fieldValues.created.push(item.created);\n  fieldValues.name.push(item.name);\n  fieldValues.status.push(item.status);\n  fieldValues.value.push(item.value);\n}\n\nconst result = {\n  ...frame,\n  fields: frame.fields.map((field) => ({\n    ...field,\n    values: fieldValues[field.name],\n  }))\n}\n\nreturn Promise.resolve(result);",
                "valuesEditor": "custom"
              }
            }
          },
          "refId": "A"
        }
